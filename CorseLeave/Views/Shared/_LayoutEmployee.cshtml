<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~//cdn.datatables.net/1.10.24/css/jquery.dataTables.min.css" />
    <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/css/sb-admin-2.min.css" rel="stylesheet">
</head>
<body>
    <div>
        @RenderBody()
    </div>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~//cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js" asp-append-version="true"></script>
    <script src="~//cdn.datatables.net/buttons/1.7.0/js/dataTables.buttons.min.js" asp-append-version="true"></script>
    <script src="~//cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js" asp-append-version="true"></script>
    <script src="~//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js" asp-append-version="true"></script>
    <script src="~//cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js" asp-append-version="true"></script>
    <script src="~//cdn.datatables.net/buttons/1.7.0/js/buttons.html5.min.js" asp-append-version="true"></script>
    <script src="~//cdn.jsdelivr.net/npm/sweetalert2@11" asp-append-version="true"></script>
    <script src="~//cdnjs.cloudflare.com/ajax/libs/Chart.js/3.2.1/chart.min.js" asp-append-version="true"></script>
    <script src="~https://code.jquery.com/jquery-3.5.1.js" asp-append-version="true"></script>

    <!-- Bootstrap core JavaScript-->
    @*<script src="~/vendor/jquery/jquery.min.js" asp-append-version="true"></script>*@
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js" asp-append-version="true"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js" asp-append-version="true"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js" asp-append-version="true"></script>

    <!-- Page level plugins -->
    <script src="~/vendor/chart.js/Chart.min.js" asp-append-version="true"></script>

    <!-- Page level custom scripts -->
    <script src="~/js/demo/chart-area-demo.js" asp-append-version="true"></script>
    <script src="~/js/demo/chart-pie-demo.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/employee.js" asp-append-version="true"></script>
</body>
</html>
